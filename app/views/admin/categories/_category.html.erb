<tr>
  <td><%= category.name %></td>
  <td><%= category.created_at.strftime format: :date_month_year_concise %></td>
  <td>
    <%= link_to edit_admin_category_path(category) do %>
      <span><i class="fa fa-edit fa-1px"></i></span>
    <% end %>
  </td>
  <td>
    <%= link_to admin_category_path(category), method: :delete, data: {confirm: t("flash.danger")} do %>
      <span><i class="fa fa-times fa-1px"></i></span>
    <% end %>
  </td>
</tr>
